</head>

<body>
	<div class="wrapper">

		<!--=== Search Block Version 2 ===-->
		<div id="searchBox">
			<div class="container">
				<div class="col-md-6 col-md-offset-3">
					<div class="input-group">
						<span class="input-group-btn">
							<button id="doAdv" class="btn-u btn-u-default" type="button"><i class="fa fa-cogs"></i></button>
						</span>
						<input id="qt" type="text" class="form-control" placeholder="Cerca...">
						<span class="input-group-btn">
							<button id="doSearch" class="btn-u" type="button"><i class="fa fa-search"></i></button>
						</span>
					</div>

					<div id="adv" class="panel-group acc-v1 animated fadeInDown" style="display:none;margin-top:8px;">
						<!-- CATEGORIE -->
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#adv" href="#collapse-One" aria-expanded="false">
										Categorie										
									</a>									
								</h4>
							</div>							
							<div id="collapse-One" class="panel-collapse collapse" aria-expanded="false" style="height: 0px;">
								<div class="panel-body">
									<div class="row" id="catDrop">
										<div class="col-md-4" data-cp-cbox-pos="0"></div>
										<div class="col-md-4" data-cp-cbox-pos="1"></div>
										<div class="col-md-4" data-cp-cbox-pos="2"></div>
									</div>
								</div>
							</div>						
						</div>
						<!-- PERIODO -->
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#adv" href="#collapse-Time" aria-expanded="false">
										Periodo <span id="advDate" style="font-style: italic;"></span>
									</a>									
								</h4>
							</div>
							<div id="collapse-Time" class="panel-collapse collapse" aria-expanded="false" style="height: 0px;">
								<div class="panel-body">

									<div class="tab-v2">
										<ul class="nav nav-tabs">											
											<li class="active"><a href="#profile-1" class="advTimeMenu" data-cp-tm="d-int" aria-expanded="true" data-toggle="tab" aria-expanded="false">Intervallo</a></li>
											<li class=""><a href="#messages-1" class="advTimeMenu" data-cp-tm="d-ship" data-toggle="tab" aria-expanded="false">Prossimo arrivo</a></li>
										</ul>
										<div class="tab-content">
											<div class="tab-pane fade active in" id="profile-1">								
												<div id="intdates" class="adv">
											        <div class="form-group">
											            <div class='input-group date' id='dtp1'>
											                <input type='text' class="form-control" readonly placeholder="Data inizio..." />
											                <span class="input-group-addon">
											                    <span class="fa fa-calendar"></span>
											                </span>
											            </div>
											        </div>
										            <div class="form-group">
											            <div class='input-group date' id='dtp2'>
											                <input type='text' class="form-control" readonly placeholder="Data fine..." />
											                <span class="input-group-addon">
											                    <span class="fa fa-calendar"></span>
											                </span>		                
											            </div>
											        </div>
											        <div class="text-right">
											        	<button class="btn-u btn-u-green cp-ok" type="button"><i class="fa fa-check"></i> Imposta</button>
											        	<button class="btn-u btn-u-red cp-undo" type="button"><i class="fa fa-undo"></i> Reset</button>
											        </div>
												</div>
											</div>
											<div class="tab-pane fade" id="messages-1">								
												<input class="typeahead form-control adv" placeholder="Nome nave">
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="panel panel-default">
							<div class="panel-heading">
								<h4 class="panel-title">
									<a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#adv" href="#collapse-Map" aria-expanded="false">
										Posizione <span style="font-style: italic;"></span>
									</a>									
								</h4>
							</div>
							<div id="collapse-Map" class="panel-collapse collapse" aria-expanded="false" style="height: 0px;">
								<div class="panel-body">									
									<div id="map" class="map margin-bottom-30"></div>
									<div class="sky-form label-rounded">										
										<label class="label rounded-x">Seleziona un raggio (<span id="slider1-value-rounded">0</span>m)</label>
										<div id="slider1-rounded" class="skyforms"></div>
									</div>
								</div>
							</div>
						</div>
					</div><!-- end adv -->

				</div>
      		</div>
		</div><!-- end searchtool -->


		<!--=== Search Results ===-->
		<div class="container s-results margin-bottom-50">
			<span class="results-number">About <span id="tot"></span> results</span>
			<div id="searchresults"></div>
			<div class="margin-bottom-30"></div>
		</div><!--/container-->
		<!--=== End Search Results ===-->

	</div><!--/End Wrapper-->


	<script type="text/javascript">
		var baseUrl = '<%= baseUrl %>';
		var contentsUrl = '<%= contentsUrl %>';
	</script>
	<!-- JS Global Compulsory -->
