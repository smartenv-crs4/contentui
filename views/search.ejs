	<script type="text/javascript" src="assets/js/app.js"></script>

	<script type="text/javascript" src="js/search.js"></script>

	<!--[if lt IE 9]>
	<script src="assets/plugins/respond.js"></script>
	<script src="assets/plugins/html5shiv.js"></script>
