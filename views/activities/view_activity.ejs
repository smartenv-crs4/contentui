                    <i class="icon-custom rounded fa fa-{{this.icon}}"></i>
                    <h2 class="heading-md">{{this.name}}</h2>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
    <!--=== End Content Part ===-->

